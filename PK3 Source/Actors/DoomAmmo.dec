// Energy
ACTOR Energy : AMMO
{
	//$Category Final Duel-Ammunition
	Tag "Soul Energy"
	Game Doom
	Inventory.PickupMessage ""
	Inventory.Amount 1
	Inventory.MaxAmount 100
	Ammo.BackpackAmount 1
	Ammo.BackpackMaxAmount 100
