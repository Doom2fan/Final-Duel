ACTOR FDBackpack : CustomInventory 30022
{
	//$Category Final Duel-Ammunition
	Tag "Backpack"
	Game Doom
	Height 26
	Inventory.PickupMessage "You got a backpack."
	Inventory.PickupSound "misc/backpack"
	States
	{
	Spawn: // Sprites by ID Software, edited by Alex_Mercer
		BPAK A -1
		Stop
    Pickup:
		TNT1 A 0 A_GiveInventory("BackpackItem", 1)
        TNT1 A 0 A_GiveInventory("ShellBox2", 4)
        TNT1 A 0 A_GiveInventory("ClipBox2", 4)
        TNT1 A 0 A_GiveInventory("CellPack2", 4)
        TNT1 A 0 A_GiveInventory("RocketBox2", 4)
	    TNT1 A 0 A_GiveInventory("HellClipBox", 4)
		TNT1 A 0 A_GiveInventory("MinigunAmmo", 4)
        Stop
	}
}
